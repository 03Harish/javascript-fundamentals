let number=1;
if (number>0)
{console.log("positive number");
}
else if(number<0){
console.log("negative number");}
else{
    console.log("number is zero")
}



 let avg=87;
 if(avg>=91 && avg<=100)
 {
    console.log(" Grade A ")
 }
 else if(avg>=81 && avg<=90)
 {
    console.log(" Grade B ")
 }
 else if(avg>=71 && avg<=80)
 {
    console.log(" Grade C ")
 }
 else
 {
    console.log(" Grade D ")
 }