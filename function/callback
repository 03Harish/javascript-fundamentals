//high order fn

function mycallback(){
    console.log("this is a callback function");
}
function test(fun){
    fun();
}
test(mycallback);


//setTimedout function

setTimeout(function(){
    console.log('hello world');
},3000);

setInterval(function(){
    console.log('youtube');
},3000); 

const numbers=[1,2,3];
numbers.forEach(function(number){
    console.log(number);
});


